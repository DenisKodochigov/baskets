<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.shopping_list.ui.product.ProductFragment"
        android:label="@string/title_box"
        tools:layout="@layout/fragment_product" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.shopping_list.ui.box.BoxFragment"
        android:label="@string/title_products"
        tools:layout="@layout/fragment_box" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.shopping_list.ui.settings.SettingsFragment"
        android:label="@string/title_settings"
        tools:layout="@layout/fragment_settings" />
</navigation>